{
  "name": "Paranoi4k Extension",
  "description": "Extension officiel de @paranoi4k, soyez averti lorsqu'un live Twitch est lanc√©",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "identity",
    "storage",
    "alarms",
    "notifications",
    "tabs",
    "unlimitedStorage"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "update_url": "https://clients2.google.com/service/update2/crx"
}
