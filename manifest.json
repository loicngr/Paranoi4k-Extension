{
  "name": "Twitch Extension Login Exemple",
  "description": "Twitch extension login exemple",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "identity",
    "storage",
    "alarms",
    "notifications",
    "unlimitedStorage"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "icons": {
    "48": "icons/icon.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html"
}
